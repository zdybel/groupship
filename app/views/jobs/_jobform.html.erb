<% if current_user %>
	<%= form_for @job do |f| %>
	<%= f.label :origin %>:
	<%= f.text_field :origin %><br />

	<%= f.label :destination %>:
	<%= f.text_field :destination %><br />

	<%= f.label :description %>:
	<%= f.text_field :description %><br />

	<%= f.label :container_amount %>:
	<%= f.text_field :container_amount %><br />

	<%= f.label :cost %>:
	<%= f.text_field :cost %><br />

	<%= f.hidden_field :user_id, value: current_user.id %><br />

	<%= f.submit %>
	<% end %>
<% end %>